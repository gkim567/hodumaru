:vpi_time_precision - 12;
:vpi_module "system";
:vpi_module "vhdl_sys";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_00000000001fd390 .scope module, "tb_nand4" "tb_nand4" 2 2;
 .timescale -9 -12;
v00000000001f7610_0 .var "a", 3 0;
v00000000001f76b0_0 .var/i "i", 31 0;
v00000000001f7750_0 .net "y", 0 0, v00000000001fd5b0_0;  1 drivers
S_00000000001f7490 .scope module, "tb" "nand4_if" 2 7, 3 1 0, S_00000000001fd390;
 .timescale 0 0;
    .port_info 0 /INPUT 4 "a"
    .port_info 1 /OUTPUT 1 "y"
v00000000001fd510_0 .net "a", 3 0, v00000000001f7610_0;  1 drivers
v00000000001fd5b0_0 .var "y", 0 0;
E_00000000028341a0 .event edge, v00000000001fd510_0;
    .scope S_00000000001f7490;
T_0 ;
    %wait E_00000000028341a0;
    %load/vec4 v00000000001fd510_0;
    %cmpi/e 15, 0, 4;
    %jmp/0xz  T_0.0, 4;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v00000000001fd5b0_0, 0, 1;
    %jmp T_0.1;
T_0.0 ;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v00000000001fd5b0_0, 0, 1;
T_0.1 ;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_00000000001fd390;
T_1 ;
    %vpi_call 2 12 "$dumpfile", "test_out.vcd" {0 0 0};
    %vpi_call 2 13 "$dumpvars", 32'sb11111111111111111111111111111111, S_00000000001f7490 {0 0 0};
    %vpi_call 2 14 "$monitor", "%b", v00000000001f7750_0 {0 0 0};
    %end;
    .thread T_1;
    .scope S_00000000001fd390;
T_2 ;
    %pushi/vec4 0, 0, 4;
    %store/vec4 v00000000001f7610_0, 0, 4;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v00000000001f76b0_0, 0, 32;
T_2.0 ;
    %load/vec4 v00000000001f76b0_0;
    %cmpi/s 32, 0, 32;
    %jmp/0xz T_2.1, 5;
    %delay 20000, 0;
    %load/vec4 v00000000001f76b0_0;
    %pad/s 4;
    %store/vec4 v00000000001f7610_0, 0, 4;
    %load/vec4 v00000000001f76b0_0;
    %addi 1, 0, 32;
    %store/vec4 v00000000001f76b0_0, 0, 32;
    %jmp T_2.0;
T_2.1 ;
    %vpi_call 2 26 "$finish" {0 0 0};
    %end;
    .thread T_2;
